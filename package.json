{
    "name": "liquidtest",
    "version": "1.0.1",
    "author": "julio.freitas",
    "description": "generator pdf with puppeteer",
    "license": "ISC",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
