                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd" clip-rule="evenodd"
                                d="M10 0C4.47715 0 0 4.47715 0 10C0 15.5228 4.47715 20 10 20C15.5228 20 20 15.5228 20 10C20 7.34784 18.9464 4.8043 17.0711 2.92893C15.1957 1.05357 12.6522 0 10 0ZM10 15C9.44771 15 9 14.5523 9 14C9 13.4477 9.44771 13 10 13C10.5523 13 11 13.4477 11 14C11 14.5523 10.5523 15 10 15ZM10 12C10.5523 12 11 11.5523 11 11V6C11 5.44772 10.5523 5 10 5C9.44771 5 9 5.44772 9 6V11C9 11.5523 9.44771 12 10 12Z"
                                fill="#FF4842" />
                        </svg>
                    </div>
                    {{data.moderator.description}}
                </div>
            </div>
        </section>
        {% endif %}
        <section class="mb-10">
            <div class="bg-white shadow-md rounded-lg flex p-6 flex-col">
                <h5 class="text-sm font-bold col-span-2 text-center pb-6 text-gray-600">
                    AA {{translate.average_ratings.evaluation_deadline}} {{data.average_ratings.start_date}}
                    {{data.average_ratings.end_date}}
                </h5>
                <div class="grid grid-cols-2">
                    <div class="flex flex-col items-center justify-around">
                        <h2 class="text-center font-semibold text-gray-700">
                            {{translate.average_ratings.collaborator_note_title}}
                        </h2>
                        <h2 class="text-center mb-2 text-gray-600">
                            {{translate.average_ratings.collaborator_note_subtitle}}</h2>
                        <div class="rounded-full flex items-center justify-center text-gray-400 w-[170px] h-[170px] bg-primary-700"
                            style="background-image:conic-gradient(transparent{{data.average_ratings.evaluated_average | prepend:" "}},currentcolor 0)">
                            <div class="rounded-full w-[140px] h-[140px] flex items-center justify-center bg-white">

                         <h3 class="text-3xl font-bold text-primary-700">
                                    {{data.average_ratings.evaluated_average}}
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="flex flex-col items-center justify-around">
                        <h2 class="text-center font-semibold text-gray-700">
                            {{translate.average_ratings.position_note_title}}
                        </h2>
                        <h2 class="text-center mb-2 text-gray-600">{{translate.average_ratings.position_note_subtitle}}
                        </h2>
                        <div class="rounded-full flex items-center justify-center text-gray-400 w-[170px] h-[170px] bg-primary-700"
                            style="background-image:conic-gradient(transparent{{data.average_ratings.position_average | prepend:" "}},currentcolor 0)">
                            <div class="rounded-full w-[140px] h-[140px] flex items-center justify-center bg-white">
                                <h3 class="text-3xl font-bold text-primary-700">
                                    {{data.average_ratings.position_average}}
                                </h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="mt-10 break-before-page break-after-page">
            <div class="bg-white shadow-md rounded-lg p-6 flex flex-col">
                <h5 class="break-inside-avoid font-body font-bold text-sm mb-6 text-gray-800">
                    {{translate.skill_ratings.title}}</h5>
                {% for skill_group in data.skill_ratings.skill_group %}
                <div class="flex flex-row items-center space-x-3 items-center ml-6">
                    <h2
                        class="max-w-[80%] break-inside-avoid font-body text-xs font-bold text-primary text-ellipsis overflow-hidden break-all">
                        {{skill_group.name }}
                    </h2>
                    <div class="w-0.5 h-6 bg-gray-300"></div>
                    <span class="w-[170px] break-inside-avoid font-body text-subtitle2 font-semi-bold text-gray-600">
                        {{translate.skill_ratings.skill_group_subtitle}} {{skill_group.average}}
                    </span>
                </div>
                <div class="divide-y divide-dashed divide-gray-300">
                    {% for skill in data.skill_ratings.skill %}
                    {% if skill_group.id == skill.id %}
                    <div class="flex flex-row space-x-8 pb-6 mb-4 break-inside-avoid-page">
                        <div class="flex flex-col w-full mt-2">
                            {%if skill.evaluated_average != "0%" %}
                            <div class="flex flex-col w-full px-12">
                                <h5
                                    class="break-inside-avoid font-body text-subtitle1 font-semi-bold text-gray-800 mb-1 text-ellipsis overflow-hidden break-all">
                                    {{skill.name }}
                                </h5>
                                <p class="text-gray-600 text-body2 mb-6">
                                    {{skill.description | newline_to_br}}
                                </p>
                                <div class="flex flex-row items-center">
                                    <div class="flex flex-col w-full mr-8">
                                        <div class="flex flex-row items-center justify-between w-full mr-8">
                                            <div class="flex flex-col space-y-3 w-full">
                                                {% for rates in data.skill_ratings.rates %}
                                                {% if rates.id == skill.rate_id %}
                                                <div class="flex flex-row items-center">
                                                    <span class="w-[100px] text-caption mr-3">{{rates.evaluator}}</span>
                                                    <div class="w-[170px] h-10 relative bg-gray-300">
                                                        <div
                                                            class="h-10 relative flex items-center {{rates.bg}} w-[{{rates.width}}]">
                                                            <span
                                                                class="absolute left-4 text-caption font-regular">{{rates.rate}}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                {% endif %}
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="flex flex-col w-full">
                                        {% if skill.evaluated_average != "0%" %}
                                        <div class="flex flex-row items-center w-full justify-between mb-4">
                                            <h2 class="w-[100px] text-caption mr-3">
                                                {{translate.skill_ratings.position_general_note}}
                                            </h2>
                                            <div class="w-[170px] h-10 relative bg-gray-300">
                                                <div
                                                    class="h-10 flex items-center bg-purpura-500 w-[{{skill.evaluated_average}}]">
                                                    <span
                                                        class="absolute left-4 text-caption font-regular">{{skill.evaluated_average}}</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="flex flex-row items-center w-full justify-between">
                                            <h2 class="w-[100px] text-caption mr-3">
                                                {{translate.skill_ratings.position_note_average}}
                                            </h2>
                                            <div class="w-[170px] h-10 relative bg-gray-300">
                                                <div
                                                    class="h-10 flex items-center bg-purpura-400 w-[{{skill.position_average}}]">
                                                    <span
                                                        class="absolute left-4 text-caption font-regular">{{skill.position_average}}</span>
                                                </div>
                                            </div>
                                        </div>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                            {% endif %}
                        </div>
                    </div>
                    {% endif %}
                    {% endfor %}
                </div>
                {% endfor %}
            </div>
        </section>
        {% if data.comments.size > 0 %}
        <section class="mt-10">
            <div class="bg-white shadow-md rounded-lg flex flex-col">
                <h5 class="font-bold text-sm p-6 border-b border-gray-300 text-gray-800">
                    {{translate.comments.title}}
                </h5>
                <div class="divide-y divide-gray-300">
                    {% for comments in data.comments %}
                    <div class="break-inside-avoid py-6">
                        <div class="px-6">
                            <h5 class="font-semibold text-base text-gray-800">{{comments.evaluator}}</h5>
                            <span class="mb-2 text-caption font-regular">{{comments.date}}</span>
                            <p class="text-body2 mt-2 text-gray-600">{{comments.description}}</p>
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </section>
        {% endif %}
    </main>
</body>

</html>
