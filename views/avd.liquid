                    <div class="flex flex-col">
                        <h5 class="text-sm font-bold">
                            {{data.cicle.name}}
                        </h5>
                        <h2 class="text-subtitle1 font-semibold text-gray-600">
                            {{data.cicle.pattern}}
                        </h2>
                    </div>
                    <div>
                        {% case data.cicle.status %}
                        {% when "active" %}
                        <div
                            class="py-1 px-2 text-caption font-regular rounded-md justify-center items-center bg-info-500/[0.16] text-info-700">
                            {{translate.cicle.status}}
                        </div>
                        {% when "finished" %}
                        <div
                            class="py-1 px-2 text-caption font-regular rounded-md justify-center items-center bg-success-500/[0.16] text-success-700">
                            {{translate.cicle.status}}
                        </div>
                        {% when "canceled" %}
                        <div
                            class="py-1 px-2 text-caption font-regular rounded-md justify-center items-center bg-error-500/[0.16] text-error-700">
                            {{translate.cicle.status}}
                        </div>
                        {% when "scheduled" %}
                        <div
                            class="py-1 px-2 text-caption font-regular rounded-md justify-center items-center bg-warning-500/[0.16] text-warning-700">
                            {{translate.cicle.status}}
                        </div>
                        {% endcase %}
                    </div>
                </div>
